<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
 <extension
         id="OdysseusTestApplication"
         point="org.eclipse.core.runtime.applications">
      <application
            cardinality="singleton-global"
            thread="main"
            visible="true">
         <run
               class="de.uniol.inf.is.odysseus.test.runner.TestRunnerApplication">
         </run>
      </application>
   </extension>
   <extension
         id="odysseustestproduct"
         point="org.eclipse.core.runtime.products">
      <product
            application="de.uniol.inf.is.odysseus.test.OdysseusTestApplication"
            name="Test Runner Product">
         <property
               name="appName"
               value="Test Runner Product">
         </property>
      </product>
   </extension>
</plugin>
