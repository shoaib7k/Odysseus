<?xml version="1.0" encoding="UTF-8"?>
<Configuration shutdownHook="disable" status="error" monitorInterval="5">
    <Properties>
        <Property name="APP_LOG_ROOT">logs</Property>
    </Properties>
    <Loggers>
        <Root level="trace">
            <AppenderRef ref="Console" level="trace"/>
        </Root>
    </Loggers>
</Configuration>